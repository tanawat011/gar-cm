{
  "name": "gar-cm2",
  "description": "Gu Ask Real - Coding Man",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:gen": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:pull": "dotenv -e .env.local -- prisma db pull",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "pre:commit": "yarn lint",
    "pre:push": "yarn test:cv",
    "initial": "yarn husky && yarn db:gen",
    "dev": "next dev",
    "build": "next build",
    "build:prd": "prisma generate && yarn build",
    "start": "next start",
    "lint": "next lint",
    "commit": "git add . && git cz",
    "release": "commit-and-tag-version",
    "release:first": "commit-and-tag-version --first-release",
    "release:major": "commit-and-tag-version --release-as major",
    "release:minor": "commit-and-tag-version --release-as minor",
    "release:patch": "commit-and-tag-version --release-as patch"
  },
  "dependencies": {
    "@nextui-org/react": "^2.2.10",
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "flag-icons": "^7.2.0",
    "framer-motion": "^11.0.14",
    "lottie-react": "^2.4.0",
    "next": "14.1.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "autoprefixer": "^10.0.1",
    "commit-and-tag-version": "^12.2.0",
    "commitizen": "^4.3.0",
    "cssnano": "^6.1.0",
    "cz-git": "^1.9.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.js"
    }
  }
}
