{
  "name": "gar-cm",
  "description": "Gu Ask Real - Coding Man",
  "version": "0.5.7",
  "private": true,
  "scripts": {
    "i:all": "pnpm i --shamefully-hoist=true",
    "dev": "pnpm husky && pnpm db:gen && next dev",
    "build": "next build",
    "build:prd": "prisma generate && pnpm build",
    "start": "next start",
    "lint": "next lint",
    "husky": "npx husky install",
    "husky:add": "husky add",
    "pre:commit": "pnpm lint",
    "pre:push": "pnpm test:cv",
    "test": "jest --maxWorkers=10",
    "test:w": "pnpm run test --watchAll",
    "test:cv": "pnpm run test --coverage",
    "test:cv:msg": "pnpm run test:cv --verbose",
    "test:ez": "/bin/sh ./scripts/jest.sh",
    "test:cv:w": "pnpm run test:w --coverage",
    "test:cv:msg:w": "pnpm run test:cv:w --verbose",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:gen": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:pull": "dotenv -e .env.local -- prisma db pull",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "commit": "git add . && git cz",
    "release": "commit-and-tag-version",
    "release:first": "commit-and-tag-version --first-release",
    "release:major": "commit-and-tag-version --release-as major",
    "release:minor": "commit-and-tag-version --release-as minor",
    "release:patch": "commit-and-tag-version --release-as patch"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@auth0/nextjs-auth0": "^3.5.0",
    "@nextui-org/react": "^2.2.9",
    "@pothos/core": "^3.40.1",
    "@pothos/plugin-prisma": "^3.63.0",
    "@pothos/plugin-relay": "^3.45.1",
    "@prisma/client": "^5.7.0",
    "@reduxjs/toolkit": "^1.9.7",
    "autoprefixer": "10.4.15",
    "axios": "^1.6.5",
    "flag-icons": "^7.0.2",
    "framer-motion": "^10.16.5",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^5.0.2",
    "jsonwebtoken": "^9.0.2",
    "lottie-react": "^2.4.0",
    "next": "13.5.6",
    "next-themes": "^0.2.1",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.11.0",
    "react-redux": "^8.1.3",
    "react-syntax-highlighter": "^15.5.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.19",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-mdx-gfm": "^7.5.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling-webpack": "^0.0.5",
    "@storybook/addon-themes": "^7.5.1",
    "@storybook/blocks": "^7.5.1",
    "@storybook/nextjs": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "clsx": "^2.0.0",
    "commit-and-tag-version": "^12.0.0",
    "commitizen": "^4.3.0",
    "cssnano": "^6.0.1",
    "cz-git": "^1.8.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.7.0",
    "storybook": "^7.5.1",
    "storybook-addon-themes": "^6.1.0",
    "styled-components": "^6.1.1",
    "ts-node": "^10.9.2",
    "twin.macro": "^3.4.0",
    "typescript": "5.2.2"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "browserslist": [
    "chrome 64",
    "edge 79",
    "firefox 67",
    "opera 51",
    "safari 12"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.js"
    }
  }
}
